<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
   http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
   
   <servlet>
        <servlet-name>ServLogin</servlet-name>
        <servlet-class>claire.hrt.wiki.server.admin.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServLogin</servlet-name>
        <url-pattern>/admin/login.jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServAdminIndex</servlet-name>
        <servlet-class>claire.hrt.wiki.server.admin.AdminIndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServAdminIndex</servlet-name>
        <url-pattern>/admin/index.jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServLogout</servlet-name>
        <servlet-class>claire.hrt.wiki.server.admin.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServLogout</servlet-name>
        <url-pattern>/admin/logout.jsp</url-pattern>
    </servlet-mapping>
   
</web-app>